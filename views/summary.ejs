    <style>
        body {
            font-size: 12px;
        }

        .pd-wrapper {
            font-size: 14px;
        }

        p {
            margin: 0;
            padding: 0;
        }

        .btn {
            margin-right: 5px;
        }

        #startTime, #endTime {
            margin-right: 0px!important;
        }

        #toolbar {
            height: 90px;
            line-height: 40px;
            font-size: 12px;
            padding-bottom: 5px;
        }

        #toolbar span, #toolbar input, #toolbar div, #toolbar button, #toolbar select {
            vertical-align: middle;
            margin-right: 10px;
            font-size: 12px;
        }

        .input-medium2 {
            width: 150px;
        }

        .input-medium {
            width: 120px;
        }

        #alertPanel {
            display: none;
        }

        .important-info {
            padding-top: 20px;
        }

        #appArea {
            overflow-x: hidden;
            overflow-y: auto;
        }
