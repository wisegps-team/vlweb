<body>

<!-- 头部 -->
<% include html/header.html %>

<div class="container-fluid container-fluid2">
    <div id="alert" class="alert alert-block alert-error fade in">
        <!--<button type="button" class="close" data-dismiss="alert">&times;</button>-->
        <span id="err_msg"></span>
    </div>
    <div class="row-fluid" id="appArea">
        <!--/span-->
        <div class="important-info">
            <div class="pd-wrapper">
                <div class="mod-dash cont-module">
                    <div data-module="monitor">
                        <div class="dash-full-item-wrapper">
                            <div class="dash-item">
                                <p class="dash-item-hd"> 总体概况 </p>
                                <div class="dash-item-bd">
                                    <div class="check-area">
                                        <div class="fr-row module-item-monitor customer">
                                            <div class="ca-col-main">
                                                <a href="/customer" data-event="nav" data-hot="newhp.problem.jiankong">
                                                    <p class="ca-col-tit">客户</p>
                                                    <p class="ca-col-data"><span class="num-font" id="customerCounter">0</span></p>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="fr-row module-item-monitor depart">
                                            <div class="ca-col-main">
                                                <a href="/depart" data-event="nav" data-hot="newhp.problem.jiankong">
                                                    <p class="ca-col-tit">部门</p>
                                                    <p class="ca-col-data"><span class="num-font" id="departCounter">0</span></p>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="fr-row module-item-monitor employee">
                                            <div class="ca-col-main">
                                                <a href="/employee" data-event="nav" data-hot="newhp.problem.jiankong">
                                                    <p class="ca-col-tit">成员</p>
                                                    <p class="ca-col-data"><span class="num-font" id="employeeCounter">0</span></p>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="fr-row module-item-monitor vehicle">
                                            <div class="ca-col-main">
                                                <a href="/vehicle" data-event="nav" data-hot="newhp.problem.jiankong">
                                                    <p class="ca-col-tit">车辆</p>
                                                    <p class="ca-col-data"><span class="num-font" id="vehicleCounter">0</span></p>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="fr-row module-item-monitor device">
                                            <div class="ca-col-main">
                                                <a href="/device" data-event="nav" data-hot="newhp.problem.jiankong">
                                                    <p class="ca-col-tit">设备</p>
                                                    <p class="ca-col-data"><span class="num-font" id="deviceCounter">0</span></p>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div data-module="backlog">
                        <div class="dash-half-item-wrapper">
                            <div class="dash-item chart-item">
                                <p class="dash-item-hd"> 车辆里程/油耗 </p>
                                <div class="dash-item-bd">
                                    <div id="stat" style="padding-top:20px;width: 100%;height:280px;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div data-module="backlog">
                        <div class="dash-square-item-wrapper">
                            <div class="dash-item chart-item">
                                <p class="dash-item-hd"> 车辆上线统计 </p>
                                <div class="dash-item-bd">
                                    <div id="online" style="padding-top:0px;width: 100%;height:280px;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div data-module="backlog">
                        <div class="dash-square-item-wrapper">
                            <div class="dash-item chart-item">
                                <p class="dash-item-hd"> 车辆状态统计 </p>
                                <div class="dash-item-bd">
                                    <div id="status" style="padding-top:0px;width: 100%;height:280px;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/span-->
    </div>
    <!--/row-->

    <!-- 新增或者修改账户 -->
    <% include dialog/frmAccount.html %>

</div>
