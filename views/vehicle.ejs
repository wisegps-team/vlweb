<body>

    <!-- 头部 -->
    <% include html/header.html %>

        <div class="container-fluid container-fluid2">
            <div id="alert" class="alert alert-block alert-error fade in">
                <!--<button type="button" class="close" data-dismiss="alert">&times;</button>-->
                <span id="err_msg"></span>
            </div>
            <div class="row-fluid">
                <div class="span3">
                    <div class="sidebar-nav">
                        <div>
                            <input id="searchKey" type="input" placeholder="<%= t('customer.search') %>" data-i18n="[placeholder]customer.search">
                            <!--<button type="button" class="btn btn-primary" id="searchCustomer" display="none">搜索</button>-->
                        </div>
                        <!--<ul id="customer_list" class="nav nav-list customer-list">-->
                        <!--</ul>-->
                        <ul id="customerTree" class="ztree" title="<%= t('customer.click_tree') %>" data-i18n="[title]customer.click_tree">
                            <p style="text-align: center; padding-top: 5px">
                                <img style='width:16px;height:16px' src='/img/waiting.gif' />
                            </p>
                        </ul>
                    </div>
                    <!--/.well -->
                </div>
                <!--/span-->
                <div class="span9 vehicleList">
                    <div class="input-append pull-left" style="height: 32px; line-height: 32px; font-size: 14px; padding-bottom: 0px">
                        <span id="sendTo" data-i18n="customer.select_label">
                            <%= t('customer.select_label') %>
                        </span>
                        <span id="selCustName" data-i18n="customer.select_value">
                            <%= t('customer.select_value') %>
                        </span>
                    </div>
                    <div class="input-append pull-right" style="line-height: 40px">
                        <select id="searchType" class="input-small">
                            <option value="name" data-i18n="vehicle.name">
                                <%= t('vehicle.name') %>
                            </option>
                            <option value="did" data-i18n="device.id">
                                <%= t('device.id') %>
                            </option>
                            <option value="sim" data-i18n="vehicle.sim">
                                <%= t('vehicle.sim') %>
                            </option>
                            <option value="contact" data-i18n="vehicle.contact">
                                <%= t('vehicle.contact') %>
                            </option>
                            <option value="tel" data-i18n="vehicle.tel">
                                <%= t('vehicle.tel') %>
                            </option>
                        </select>
                        <input id="vehicleKey" class="input-medium" type="text" data-provide="typeahead" data-items="5" autocomplete="off" data-source=""
                            placeholder="<%= t('vehicle.search') %>" data-i18n="[placeholder]vehicle.search">
                        <!-- <button class="btn" type="button" id="searchVehicle">搜索</button> -->
                        <button class="btn btn-primary" type="button" id="addVehicle" data-i18n="vehicle.new">
                            <%= t('vehicle.new') %>
                        </button>
                        <button class="btn" type="button" id="changeParent" data-i18n="vehicle.change_parent">
                            <%= t('vehicle.change_parent') %>
                        </button>
                        <button class="btn btn-primary" type="button" id="export" data-i18n="report.export" style="display:none;margin-left:10px">
                            <%= t('report.export') %>
                        </button>
                        <!-- <button class="btn btn-primary" type="button" id="delVehicle">删除</button> -->
                        <!-- <a href="#frmVehicle" role="button" class="btn" data-toggle="modal"><i class="icon-plus"></i> 新增</a>
                <a href="#" role="button" class="btn"><i class="icon-remove"></i> 删除</a> -->
                    </div>
                    <div class="input-append pull-right" style="height: 26px; line-height: 26px; font-size: 12px">
                        <input id="allNode" type="checkbox" style="vertical-align: middle; margin-bottom: 3px">
                        <span style="vertical-align: middle; padding: 5px 20px 5px 2px" data-i18n="customer.contain_nodes">
                            <%= t('customer.contain_nodes') %>
                        </span>
                    </div>
                    <div id="vehicle_panel">
                        <table class="table table-hover table-striped table-bordered" width="100%" cellpadding="0" cellspacing="0" border="0" id="vehicle_list">
                            <thead>
                                <tr class="tops">
                                    <th width="20px" style="text-align: center !important;">
                                        <input id="checkAll" type="checkbox">
                                    </th>
                                    <th style="text-align: center !important;" data-i18n="vehicle.name">
                                        <%= t('vehicle.name') %>
                                    </th>
                                    <th width="15%" data-i18n="device.id">
                                        <%= t('device.id') %>
                                    </th>
                                    <!--<th width="12%">终端条码</th>-->
                                    <th width="15%" data-i18n="vehicle.sim">
                                        <%= t('vehicle.sim') %>
                                    </th>
                                    <th width="90px" data-i18n="vehicle.contact">
                                        <%= t('vehicle.contact') %>
                                    </th>
                                    <th width="90px" data-i18n="vehicle.tel">
                                        <%= t('vehicle.tel') %>
                                    </th>
                                    <th width="90px" data-i18n="vehicle.create_date">
                                        <%= t('vehicle.create_date') %>
                                    </th>
                                    <th width="90px" data-i18n="vehicle.end_date">
                                        <%= t('vehicle.end_date') %>
                                    </th>
                                    <th width="90px" data-i18n="vehicle.op">
                                        <%= t('vehicle.op') %>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
                <!--/span-->
            </div>
            <!--/row-->

            <!-- 新增或者修改车辆 -->
            <% include dialog/frmVehicle.html %>

                <!-- 新增或者修改账户 -->
                <% include dialog/frmAccount.html %>

                    <!-- 更换用户 -->
                    <% include dialog/frmCustomerAssign.html %>

                        <!-- 选择部门 -->
                        <% include dialog/frmDepartAssign.html %>

        </div>
        <!--/.fluid-container-->
        <!-- 版本信息 -->
        <% include html/footer.html %>

